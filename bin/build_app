#!/usr/bin/env bash

# Build cybele gem
echo "Build cybele gem"
./bin/build_gem

# Remove example
echo "Remove test"
rm -rf tmp/example

# Disable spring
export DISABLE_SPRING=1
# or you can run this command
# ps ax | grep spring | grep dummy_app | cut -f1 -d' ' | xargs kill

# Create dummy_app
echo "Create example"
cybele tmp/example --skip-ask